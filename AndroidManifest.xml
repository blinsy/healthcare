<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.healthcareunit"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.healthcareunit.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.healthcareunit.MainActivity"
            android:label="Home page"></activity>
        <activity android:name="com.healthcareunit.Easysearch"
            android:label="Easy search page"></activity>
        <activity android:name="com.healthcareunit.Ailements"
            android:label="Ailements page"></activity>
        <activity android:name="com.healthcareunit.Mother"
            android:label="Mother and child page"></activity>
        <activity android:name="com.healthcareunit.Consulting"
            android:label="Consulting page"></activity>
        <activity android:name="com.healthcareunit.Reminder"
            android:label="Notification and Reminder page"></activity>
        <activity android:name="com.healthcareunit.Settings"
            android:label="Profile and settings page"></activity>
        <activity android:name="com.healthcareunit.Antenatal"
            android:label="Antebatal page"></activity>
        <activity android:name="com.healthcareunit.Postnatal"
            android:label="Postnatal page"></activity>
        <activity android:name="com.healthcareunit.Babycard"
            android:label="Baby's card page"></activity>
        <activity android:name="com.healthcareunit.Fertility"
            android:label="Fertility page"></activity>
        <activity android:name="com.healthcareunit.Practitioner"
            android:label="Practitioner page"></activity>
        <activity android:name="com.healthcareunit.Remind"></activity>
        <activity android:name="com.healthcareunit.Notification"></activity>
    </application>

</manifest>